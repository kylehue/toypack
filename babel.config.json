{
	"presets": ["@babel/preset-env", "@babel/preset-typescript"],
	"plugins": [
		[
			"module-resolver",
			{
				"root": ["./src"],
				"alias": {
					"@toypack": "./src",
					"fs": "memfs"
				}
			}
		],
      [
         "dynamic-import-node",
         {
            "noInterop": true
         }
      ]
	],
	"sourceMaps": "inline",
	"ignore": ["./src/*.d.ts"]
}
