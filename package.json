{
	"name": "toypack",
	"version": "0.0.1",
	"description": "A sandbox bundler for browsers.",
	"main": "./lib/main.umd.js",
	"typings": "./lib/core/Toypack.d.ts",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kylehue/toypack.git"
	},
	"homepage": "https://github.com/kylehue/toypack#readme",
	"scripts": {
		"dev": "concurrently \"rimraf lib\" \"npm:build:types\" \"npm:build:js -- -w\"",
		"build": "concurrently \"rimraf lib\" \"npm:build:types\" \"npm:build:js\"",
		"build:types": "tsc --emitDeclarationOnly && tsc-alias",
		"build:js": "babel src --out-dir lib --extensions \".ts,.tsx\"",
		"webpack": "webpack --config webpack/webpack.common.js",
		"publish": "npm run build && npm publish"
	},
	"author": "kylehue",
	"license": "ISC",
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"lib/"
	],
	"keywords": [
		"build",
		"build tool",
		"bundle",
		"bundler",
		"sandbox"
	],
	"devDependencies": {
