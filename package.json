		"dts-bundle-webpack": "^1.0.2",
		"dts-generator": "^3.0.0",
		"ignore-loader": "^0.1.2",
		"node-polyfill-webpack-plugin": "^2.0.1",
		"npm-dts-webpack-plugin": "^1.3.12",
		"sass-loader": "^13.0.2",
		"style-loader": "^3.3.1",
		"terser-webpack-plugin": "^5.3.6",
