{
   "name": "toypack",
   "description": "A sandbox bundler for browsers.",
   "version": "1.0.0",
   "type": "module",
   "types": "./build/Toypack.d.ts",
   "license": "MIT",
   "author": "kylehue",
   "exports": {
      ".": "./build/Toypack.js",
      "./utils": "./build/utils/index.js",
      "./types": "./build/types.js"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/kylehue/toypack.git"
   },
   "homepage": "https://github.com/kylehue/toypack#readme",
   "publishConfig": {
      "access": "public"
   },
   "scripts": {
      "dev": "set NODE_ENV=development&&concurrently \"npm run build:main -- --watch\" \"npm run type:dts -- --watch\" \"cd ./example&&npm run dev\"",
      "build": "set NODE_ENV=production&&concurrently \"npm run clean\" \"npm run build:main\" \"npm run build:bundle\" \"npm run type:dts\"",
      "build:main": "node ./swc/make-swcrc.js&&swc ./src -d ./build --config-file ./swc/.swcrc",
      "build:bundle": "webpack",
      "clean": "rimraf build browser coverage nyc_output",
      "type:dts": "tsc --emitDeclarationOnly",
      "type:check": "tsc --noEmit",
      "format": "prettier \"src/**/*.ts\" --write",
      "format:check": "prettier \"src/**/*.ts\" --check",
      "test": "vitest run",
      "test:watch": "vitest watch",
      "test:coverage": "vitest run --coverage",
      "cz": "cz"
   },
   "devDependencies": {
      "@ryansonshine/commitizen": "^4.2.8",
      "@ryansonshine/cz-conventional-changelog": "^3.3.4",
      "@swc/cli": "^0.1.62",
      "@swc/core": "^1.3.52",
      "@types/babel__code-frame": "^7.0.3",
      "@types/babel__core": "^7.20.1",
      "@types/babel__traverse": "^7.20.0",
      "@types/convert-source-map": "^2.0.0",
      "@types/css-tree": "^2.3.1",
      "@types/lodash-es": "^4.17.8",
      "@types/path-browserify": "^1.0.0",
      "@types/semver": "^7.5.0",
      "@vitest/coverage-v8": "^0.32.4",
      "c8": "^7.13.0",
      "chokidar": "^3.5.3",
      "concurrently": "^8.0.1",
      "jsdom": "^21.1.1",
      "node-polyfill-webpack-plugin": "^2.0.1",
      "nyc": "^15.1.0",
      "prettier": "^2.8.7",
      "rimraf": "^4.4.1",
      "source-map-support": "^0.5.21",
      "ts-loader": "^9.4.3",
      "type-fest": "^3.11.1",
      "typescript": "^5.1.3",
      "vitest": "^0.32.4",
      "vitest-fetch-mock": "^0.2.2",
      "webpack": "^5.86.0",
      "webpack-cli": "^5.1.4"
   },
   "files": [
      "./build",
      "./browser"
   ],
   "keywords": [
      "sandbox",
      "bundler",
      "playground",
      "live",
      "browser"
   ],
   "dependencies": {
      "@babel/code-frame": "^7.22.5",
      "@babel/generator": "^7.22.9",
      "@babel/parser": "^7.22.5",
      "@babel/template": "^7.22.5",
      "@babel/traverse": "^7.22.4",
      "@babel/types": "^7.22.5",
      "@jridgewell/gen-mapping": "^0.3.3",
      "@jridgewell/trace-mapping": "^0.3.18",
      "convert-source-map": "^2.0.0",
      "css-tree": "^2.3.1",
      "lodash-es": "^4.17.21",
      "magic-string": "^0.30.2",
      "node-html-parser": "^6.1.5",
      "path-browserify": "^1.0.1",
      "semver": "^7.5.3"
   }
}
