{
	"name": "toypack",
	"version": "0.0.1",
	"description": "A sandbox bundler for browsers.",
	"main": "./lib/core/Toypack.js",
	"typings": "./lib/core/Toypack.d.ts",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kylehue/toypack.git"
	},
	"homepage": "https://github.com/kylehue/toypack#readme",
	"scripts": {
		"dev": "webpack --config webpack/webpack.dev.js",
		"build": "webpack --config webpack/webpack.prod.js",
		"publish": "npm run build && npm publish"
	},
	"author": "kylehue",
	"license": "ISC",
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"lib/"
	],
	"exports": {
		".": {
			"import": {
				"default": "./lib/core/Toypack.js"
			},
			"require": "./lib/core/Toypack.js",
			"types": "./lib/core/Toypack.d.ts"
		},
		"./loaders": "./lib/loaders/index.js",
		"./loaders/*": "./lib/loaders/*"
	},
	"keywords": [
		"build",
		"build tool",
		"bundle",
		"bundler",
		"sandbox"
	],
	"devDependencies": {
		"@types/lodash.clonedeep": "^4.5.7",
		"@types/lodash.merge": "^4.6.7",
		"glob": "^8.0.3",
		"html-webpack-plugin": "^5.5.0",
		"node-polyfill-webpack-plugin": "^2.0.1",
		"ts-loader": "^9.4.1",
		"tsc-alias": "^1.7.1",
		"typescript": "^4.9.3",
		"webpack": "^5.74.0",
		"webpack-cli": "^4.10.0",
		"webpack-dev-server": "^4.11.1",
		"webpack-merge": "^5.8.0"
	},
	"dependencies": {
		"@babel/parser": "^7.20.3",
		"@babel/standalone": "^7.20.6",
		"@babel/traverse": "^7.20.1",
		"@csstools/postcss-sass": "^5.0.1",
		"@jridgewell/sourcemap-codec": "^1.4.14",
		"@vue/compiler-sfc": "^3.2.45",
		"autoprefixer": "^10.4.13",
		"babel-minify": "^0.5.2",
		"combine-source-map": "^0.8.0",
		"convert-source-map": "^2.0.0",
		"lodash.clonedeep": "^4.5.0",
		"lodash.merge": "^4.6.2",
		"magic-string": "^0.26.7",
		"memfs": "^3.4.12",
		"merge-source-map": "^1.1.0",
		"node-html-parser": "^6.1.4",
		"postcss": "^8.4.19",
		"postcss-sass": "^0.5.0",
		"postcss-scss": "^4.0.6",
		"postcss-value-parser": "^4.2.0",
		"posthtml-parser": "^0.11.0",
		"posthtml-render": "^3.0.0",
		"sass.js": "^0.11.1",
		"semver": "^7.3.8",
		"source-map": "^0.7.4",
		"whatwg-fetch": "^3.6.2"
	}
}
