{
   "name": "toypack",
   "version": "0.0.1",
   "description": "A sandbox bundler for browsers.",
   "main": "./lib/Toypack.js",
   "typings": "./lib/index.d.ts",
   "repository": {
      "type": "git",
      "url": "git+https://github.com/kylehue/toypack.git"
   },
   "homepage": "https://github.com/kylehue/toypack#readme",
   "scripts": {
      "dev": "webpack --config webpack/webpack.dev.js",
      "build": "concurrently \"rimraf lib\" \"webpack --config webpack/webpack.prod.js\"",
      "publish": "npm run build && npm publish",
      "dev-test": "concurrently \"npm run dev\" \"jest --watchAll\"",
      "test": "jest"
   },
   "author": "kylehue",
   "license": "ISC",
   "publishConfig": {
      "access": "public"
   },
   "files": [
      "lib/"
   ],
   "exports": {
      ".": {
         "import": "./lib/Toypack.js",
         "require": "./lib/Toypack.js",
         "types": "./lib/Toypack.d.ts"
      }
   },
   "keywords": [
      "build",
      "build tool",
      "bundle",
      "bundler",
      "sandbox"
   ],
   "devDependencies": {
      "@babel/parser": "^7.20.7",
      "@babel/standalone": "^7.20.12",
      "@babel/traverse": "^7.20.12",
      "@types/babel__standalone": "^7.1.4",
      "@types/combine-source-map": "^0.8.2",
      "@types/convert-source-map": "^2.0.0",
      "@types/jest": "^29.2.4",
      "@types/lodash-es": "^4.17.6",
      "@types/path-browserify": "^1.0.0",
      "@types/shortid": "^0.0.29",
      "@vue/compiler-sfc": "^3.2.45",
      "babel-minify": "^0.5.2",
      "combine-source-map": "^0.8.0",
      "concurrently": "^7.6.0",
      "convert-source-map": "^2.0.0",
      "glob": "^8.0.3",
      "jest": "^29.3.1",
      "jest-environment-jsdom": "^29.3.1",
      "jest-mock-fetch": "^2.0.5",
      "lodash-es": "^4.17.21",
      "magic-string": "^0.27.0",
      "merge-source-map": "^1.1.0",
      "mime-types": "^2.1.35",
      "node-html-parser": "^6.1.4",
      "node-polyfill-webpack-plugin": "^2.0.1",
      "parse-package-name": "^1.0.0",
      "path-browserify": "^1.0.1",
      "postcss": "^8.4.21",
      "postcss-sass": "^0.5.0",
      "postcss-scss": "^4.0.6",
      "postcss-value-parser": "^4.2.0",
      "sass.js": "^0.11.1",
      "shortid": "^2.2.16",
      "terser-webpack-plugin": "^5.3.6",
      "ts-jest": "^29.0.3",
      "ts-loader": "^9.4.1",
      "tsc-alias": "^1.7.1",
      "typescript": "^4.9.3",
      "webpack": "^5.74.0",
      "webpack-cli": "^4.10.0",
      "webpack-merge": "^5.8.0",
      "whatwg-fetch": "^3.6.2"
   }
}
